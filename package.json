{
  "name": "doki-build-source",
  "version": "84.3",
  "description": "Building blocks for constructing the Doki Themes",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
