    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZimCodes/doki-build-source.git"
  },
  "author": "Unthottled",
  "license": "MIT",
  "homepage": "https://github.com/ZimCodes/doki-build-source#readme",
  "devDependencies": {
    "@types/node": "^24.10.2",
    "typescript": "^5.9.3"
  },
  "packageManager": "yarn@4.12.0"
}
